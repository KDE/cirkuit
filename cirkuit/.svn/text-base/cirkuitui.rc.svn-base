<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE kpartgui SYSTEM "kpartgui.dtd">
<kpartgui name="cirkuit" version="1" >
  <ToolBar name="mainToolBar" >
    <text>Main Toolbar</text>
    <Action name="showDock" />
  </ToolBar>
  <MenuBar>
    <Menu name="build" >
      <text>&amp;Build</text>
      <Action name="refreshPreview" />
		<Action name="previewEps" />
      <Separator/>
      <Action name="exportToEps" />
      <Action name="exportToPdf" />
      <Action name="exportToPng" />      
      <Action name="exportToPst" />
		<Action name="exportToPs" />
		<Action name="exportToSvg" />
    </Menu>
    
    <Menu name="view" >
      <text>&amp;View</text>
      <Action name="showDock" />
    </Menu>
    <Menu name="help" >
      <text>&amp;Help</text>
      <Action name="showManual" />
      <Action name="showExamples" />
    </Menu>
  </MenuBar>
</kpartgui>
