<?xml version="1.0" encoding="UTF-8"?>
<mime-info xmlns="http://www.freedesktop.org/standards/shared-mime-info">
  <mime-type type="application/x-cirkuit">
    <comment>Cirkuit drawing</comment>
	<comment xml:lang="it">Disegno Cirkuit</comment>
    <sub-class-of type="text/plain"/>
	 <magic priority="50">
				<match type="string" value=".PS" offset="0:500"/>
	 </magic>
	<glob pattern="*.cir" weight="100"/>
	<glob pattern="*.m4"/>	
  </mime-type>
</mime-info>
